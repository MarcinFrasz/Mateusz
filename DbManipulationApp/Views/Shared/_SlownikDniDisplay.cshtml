@model IEnumerable<SlownikDni>
<label class="text-danger">@TempData["error"]</label>
<br />
<div id="Videos_table" style="overflow:scroll; height:600px;">
<table  class="table table-hover">
    <thead>
    <tr class="table-primary">
        <th>Dzień liturgiczny</th>
        <th>Nazwa dnia</th>
        <th>Święto</th>
        <th> Timestamp</th>
    </tr>
    </thead>
    @{
            foreach(var item in Model)
            {
            <tr>
            <td>@item.DzienLiturgiczny</td>
            <td>@item.NazwaDnia</td>
            <td>@item.Swieto</td>
            <td>@item.Timestamp</td>
            <td>
            <div align="center">
            <a asp-controller="SlownikDni" asp-action="Edit" asp-route-id="@item.DzienLiturgiczny" 
            class="btn btn-warning"><i class="bi bi-pencil-square"></i> Edytuj</a>
            <br>
            <br>
            <a asp-controller="SlownikDni" asp-action="Delete" asp-route-id="@item.DzienLiturgiczny" 
            class="btn btn-danger"><i class="bi bi-trash-fill"></i> Usuń</a>
            </div>
            </td>
            </tr>
            }
        }
</table>
</div>
<br />
